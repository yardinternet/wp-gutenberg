{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "yard/collapse-item",
	"version": "0.2.0",
	"title": "Uitklap item",
	"category": "yard",
	"description": "Uitklapbaar component.",
	"attributes": {
		"hasStructuredData": {
			"type": "boolean",
			"default": false
		},
		"headingLevel": {
			"type": "string",
			"default": "h3"
		},
		"headingText": {
			"type": "string",
			"default": ""
		},
		"icon": {
			"type": "string",
			"default": ""
		},
		"iconAltText": {
			"type": "string",
			"default": ""
		},
		"isOpen": {
			"type": "boolean",
			"default": false
		},
		"anchor": {
			"type": "string",
			"default": ""
		},
		"hasSubtitle": {
			"type": "boolean",
			"default": false
		},
		"subtitleText": {
			"type": "string",
			"default": ""
		}
	},
	"parent": [ "yard/collapse" ],
	"supports": {
		"anchor": true,
		"html": false,
		"reusable": false
	},
	"textdomain": "yard",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css"
}
