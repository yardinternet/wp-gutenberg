{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "yard-gutenberg/collapse",
	"version": "0.1.0",
	"title": "Uitklap",
	"category": "yard-gutenberg",
	"icon": "arrow-down-alt2",
	"description": "Voeg uitklapbare blokken toe om inhoud in en uit te kunnen vouwen.",
	"attributes": {
		"hasStructuredData": {
			"type": "boolean",
			"default": false
		},
		"headingLevel": {
			"type": "string",
			"default": "h3"
		},
		"showMultiple": {
			"type": "boolean",
			"default": true
		}
	},
	"supports": {
		"html": false,
		"align": [ "wide", "full" ]
	},
	"textdomain": "collapse",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"viewScript": "file:./frontend.js"
}
